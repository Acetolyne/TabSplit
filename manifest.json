{
  
"manifest_version": 2,
  
"name": "Tab Split",
  
"description": "Forces each browser start page to open in a new browser window across multiple monitors.",
  
"version": "1.1.0",
  
"page_action": {
    
"default_icon": "logo.png"
  
},
  
"background": {
    
"scripts": ["ts.js"]
  
},
      
"icons": {
    
"32": "img.png",
	
"48": "img.png",
	
"128": "img.png"
  
},
  
"permissions": ["tabs"]
}